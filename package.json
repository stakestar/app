{
  "name": "app",
  "author": "StakeStar",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/stakestar/app",
  "main": "index.js",
  "scripts": {
    "start": "craco start -p",
    "build": "craco build",
    "build:serve": "craco build && serve -s ./build -p 3001",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.6.4",
    "@davatar/react": "^1.11.1",
    "@onestaree/ui-kit": "0.17.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@rromanovsky/web3-react-metamask": "0.2.0",
    "@stakestar/contracts": "0.18.3",
    "@stakestar/subgraph-client": "1.0.8",
    "@walletconnect/ethereum-provider": "^2.5.0",
    "@web3-react/coinbase-wallet": "8.0.35-beta.0",
    "@web3-react/core": "8.0.35-beta.0",
    "@web3-react/empty": "8.0.20-beta.0",
    "@web3-react/network": "8.0.27-beta.0",
    "@web3-react/walletconnect": "8.0.37-beta.0",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.7",
    "ethers": "5.7.2",
    "graphql-request": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "recharts": "^2.5.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@web3modal/standalone": "^2.2.0",
    "circular-dependency-plugin": "^5.2.2",
    "craco-esbuild": "^0.5.2",
    "dotenv-webpack": "^8.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-sorted": "^2.0.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.59.3",
    "serve": "^14.2.0",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write .",
      "yarn lint"
    ]
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "options": {
        "cacheStrategy": "directory",
        "wildcardAtStart": false,
        "extraAliases": [
          "~"
        ],
        "ignoredAliases": [],
        "bottomAliases": []
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 99"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
