{
  "name": "app",
  "author": "StakeStar",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/stakestar/app",
  "main": "index.js",
  "scripts": {
    "start": "craco start -p",
    "build": "craco build",
    "build:serve": "craco build && serve -s ./build -p 3001",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.5.1",
    "@davatar/react": "^1.11.0",
    "@onestaree/ui-kit": "0.4.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@stakestar/contracts": "0.2.2",
    "@stakestar/subgraph-client": "0.4.0",
    "@walletconnect/ethereum-provider": "^1.8.0",
    "@web3-react/coinbase-wallet": "8.0.34-beta.0",
    "@web3-react/core": "8.0.35-beta.0",
    "@web3-react/empty": "8.0.20-beta.0",
    "@web3-react/metamask": "8.0.28-beta.0",
    "@web3-react/network": "8.0.27-beta.0",
    "@web3-react/walletconnect": "8.0.35-beta.0",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
    "ethers": "^5.7.0",
    "graphql-request": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.14"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "circular-dependency-plugin": "^5.2.2",
    "craco-esbuild": "^0.5.2",
    "dotenv-webpack": "^8.0.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-sorted": "^2.0.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.54.6",
    "serve": "^14.0.1",
    "typescript": "^4.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write .",
      "yarn lint"
    ]
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "options": {
        "cacheStrategy": "directory",
        "wildcardAtStart": false,
        "extraAliases": [
          "~"
        ],
        "ignoredAliases": [],
        "bottomAliases": []
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 99"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
